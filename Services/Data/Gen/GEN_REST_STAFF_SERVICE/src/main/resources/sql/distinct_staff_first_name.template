SELECT * FROM (SELECT ROWNUM ROW_NUMBER, COUNT(*) OVER() TOTAL_ROWS, R1.* FROM (

  SELECT * FROM (

      (SELECT DISTINCT STAFF_FIRST_NAME FROM STAFF
          WHERE STAFF_FIRST_NAME IS NOT NULL AND
          BSN_ENT_ID = ? ORDER BY STAFF_FIRST_NAME ASC)
  )

) R1)

WHERE ROW_NUMBER BETWEEN %d AND %d
