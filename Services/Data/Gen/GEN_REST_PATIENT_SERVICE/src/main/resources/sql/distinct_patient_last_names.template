SELECT * FROM (SELECT ROWNUM ROW_NUMBER, COUNT(*) OVER() TOTAL_ROWS, R1.* FROM (

  SELECT * FROM (

      (SELECT DISTINCT PATIENT_LAST_NAME FROM PATIENT
          WHERE PATIENT_LAST_NAME IS NOT NULL AND
          BSN_ENT_ID = ? ORDER BY PATIENT_LAST_NAME ASC)
  )

) R1)

WHERE ROW_NUMBER BETWEEN %d AND %d
